  <%- include ('./partials/footer') %>
  <script>
    window.onload = function () {
    	CanvasJS.addColorSet("flexmonsterStyle",
                [//colorSet Array
                "#069E2D",
                "#058E3F",
                "#04773B",
                "#036016",
        ]);
        var achievement = {
    		colorSet: "flexmonsterStyle",
    		exportEnabled: true,
    		animationEnabled: true,
    		axisY:{
    			minimum: 0,
    			maximum: 100,
    		},
    		axisX: {
    			title: " ",
    			labelFontSize: 12,
    			labelWrap: true
    		},
    		data: [
    		{
    			type: "column",
    			legendText: "{label} %",
    			indexLabel: "{label} - {y}%",
    			indexLabelFontSize: 12,
    			yValueFormatString: "#0.##",
    			toolTipContent: "<b>{label}</b>: {count}({y}%)",
    			dataPoints: []
    		}
    		]
    	};
        $("#chartAchievement").CanvasJSChart(achievement);

    	var detailcec = {
    		colorSet: "flexmonsterStyle",
    		exportEnabled: true,
    		animationEnabled: true,
    		axisY:{
    			minimum: 0,
    			maximum: 100,
    		},
    		axisX: {
    			title: " ",
    			labelFontSize: 12,
    			labelWrap: true
    		},
    		data: [
    		{
    			type: "column",
    			legendText: "{label} %",
    			indexLabel: "{label} - {y}%",
    			indexLabelFontSize: 12,
    			yValueFormatString: "#0.##",
    			toolTipContent: "<b>{label}</b>: {count}({y}%)",
    			dataPoints: []
    		}
    		]
    	};
        $("#detailCec").CanvasJSChart(detailcec);
    }
  </script>
